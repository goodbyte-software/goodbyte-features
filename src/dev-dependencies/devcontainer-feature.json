{
    "id": "dev-dependencies",
    "version": "1.0.0",
    "name": "dev-dependencies",
    "description": "Installs development dependencies.",
    "dependsOn": {
        "ghcr.io/devcontainers/features/common-utils:2": {}
    },
    "options": {
        "ninja_version": {
            "type": "string",
            "enum": ["1.10.1", "1.10.2"],
            "default": "1.10.2",
            "description": "The version of ninja to use."
        },
        "device_tree_compiler": {
            "type": "boolean",
            "default": false,
            "description": "Whether to install device_tree_compiler."
        }
       
    }

}
